        {
          "name": "Gentle vocal compression",
          "description": "Subtle compression for vocals to control dynamics",
          "steps": [
            "Set Threshold (param index 1) to around -15 to -10 dB (~0.7-0.8)",
            "Set Ratio (param index 4) to 2:1 or 3:1 (~0.2-0.3)",
            "Set Attack (param index 2) to medium (~0.3-0.5 for 5-20ms)",
            "Set Release (param index 3) to auto or medium (~0.4-0.6)",
            "Adjust Output (param index 5) for makeup gain as needed"
          ]
        },
        {
          "name": "Aggressive drum compression",
          "description": "Heavy compression for punchy drums",
          "steps": [
            "Set Threshold (param index 1) to lower value (~0.5-0.6 for -20 to -15 dB)",
            "Set Ratio (param index 4) to 4:1 or higher (~0.4-0.6)",
            "Set Attack (param index 2) to fast (~0.1-0.2 for 0.1-2ms)",
            "Set Release (param index 3) to fast (~0.2-0.4 for 50-200ms)",
            "Increase Output (param index 5) to compensate for gain reduction"
